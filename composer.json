    "description": "Transforme NFe(xml) em objetos PHP (e vice-versa)",
    "keywords": ["nfephp", "nfe", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jansen Felipe",
            "email": "jansen.felipe@gmail.com"
        }
    ],
    "require": {
