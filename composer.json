{
  "name": "jansenfelipe/nfephp-serialize",
  "description": "Transforme NFe(xml) em objetos PHP (e vice-versa)",
  "keywords": ["nfephp", "nfe", "php"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jansen Felipe",
      "email": "jansen.felipe@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "jms/serializer": "xsd2php-dev as 0.18.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0@dev",
    "goetas/xsd2php":"2.*@dev",
    "goetas/xsd-reader":"2.*@dev"
  },
  "repositories": [{
    "type": "vcs",
    "url": "https://github.com/goetas/serializer.git"
  }],
  "autoload": {
    "psr-4": {
      "JansenFelipe\\NFePHPSerialize\\": "src"
    }
  },
  "minimum-stability": "dev"
}
